<?xml version="1.0" standalone='no'?>
<!DOCTYPE service-group SYSTEM "avahi-service.dtd">
<service-group>
  <name replace-wildcards="yes">ESP Test FTP Server on %h</name>
  <service>
    <type>_ftp._tcp</type>
    <port>21</port>
    <host-name>esp-test-ftpserver.local</host-name>
    <txt-record>version=1.0</txt-record>
    <txt-record>description=ESP Test FTP Server</txt-record>
    <txt-record>passive=true</txt-record>
  </service>
</service-group>
